{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "align": [
      true,
      "parameters",
      "statements"
    ],
    "arrow-parens": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "jsx-boolean-value": false,
    "member-access": false,
    "member-ordering": [
      true,
      {
        "order": [
          "public-before-private",
          "static-before-instance",
          "variables-before-functions"
        ]
      }
    ],
    "no-console": [
      true,
      "log",
      "error",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-duplicate-variable": true,
    "no-switch-case-fall-through": true,
    "no-var-requires": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "switch-default": true,
    "trailing-comma": false,
    "typedef": [
      true,
      "parameter",
      "property-declaration"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ]
  }
}
